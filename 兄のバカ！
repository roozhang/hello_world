    a1=" oooooo                       o o     o        o";
    a2=" o    o      ooooo       o  o o o     o       o o";
    a3=" oooooo     o  o  o      o  o       ooooooo   o o";
    a4="  o o      o   o   o     o   o        o   o   o o";
    a5="  o o      o   o   o    o     o       o   o    o";
    a6=" o  o  o    ooo   o    o       o     o    o";
    a7="o   oooo                           o    oo     o";
