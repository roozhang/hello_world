#include<iostream>
#include<vector>
using namespace std;

bool isprime(int a){
    if(a<=1){
        return false;
    }
    else if(a==2||a==3){
        return true;
    }
    else{
        for(int i = 2;i*i<=a;i++){
            if(a%i==0){
                return false;
            }
        }
        return true;
    }
}
int main(){
