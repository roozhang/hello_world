    for(int i = 0;i<=70;i++){
        x0000.push_back(i*10000);
    }
    x0000_sum={0,496165411,2137755325,5010567595,9160579439,14618393801,21410141065,29554036955,39067584251,49967497545,62260698721 ,75964656463,91084698693,107636264491,125626146767 ,145055999501,165936667657,188276813815,212087355307,237370888091,264129169599,292368935383,322099192475,353324329737,386038485587,420257213289,455993630135,493237354227,531994410929,572271532031,614071634843,657404345433,702263171257,748655659219,796585804821,846049043943,897054525479,949606745243,1003706645491,1059364518941,1116583315953,1175360897277,1235695707223,1297595548687,1361057359499,1426094118047,1492698554083,1560872603947,1630613611581,1701928394579,1774825271439,1849306517855,1925374413063,2003028614549,2082267884237,2163102457671,2245528312967,2329549541181,2415162623041,2502365978535,2591169323835,2681579579253,2773593236305,2867208069275,2962434170845,3059263723773,3157706693211,3257770146157,3359447521527,3462742012849,3567645721809};
    x0001_prime={2,2104743,224743,350381,479939,611957,746777,882389,1020389,1159531,1299721,1441051,1583591,1726951,1870669,2015179,2160563,2307233,2454619,2601869,2750161,2898541,3047773,3196939,3346621,3497867,3648941,3800213,3951169,4103641,4256249,4410323,4562707,47160834869877,5023309,5178053,5332531,5487707,5644033,5800139,5955043,6111617,6268303,6424961, 6581969,6738899,6895397,7052141,7210781,7368791,7528141,7685807,7844741,8003539,81630498322253,8482261,8640683,8799929,8960467,9121493,9281017,9442231,9602471,9763447,9925471,10086773,10248911,10410143,10570849};
    while(cin>>num){
        long long s;
        if(num==1){
            cout<<2<<endl;
        }
        else if(num==0){
            cout<<0<<endl;
        }
        else{
            int x=num/10000;
            if(x==0){
                s = 2;
                int i = 3;
                for(;num>1;i+=2){
                    if(isprime(i)){
                        s+=i;
                        num--;
                    }
                    else{

                    }
                }
            }
            else{
                s =x0000_sum[x];
                int i =x0001_prime[x];
                num%=10000;
                for(;num>0;i+=2){
                    if(isprime(i)){
                        s+=i;
                        num--;
                    }
                    else{

                    }
                }
            }



            cout<<s<<endl;
        }
    }


    return 0;
}
