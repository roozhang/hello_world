#include<iostream>
using namespace std;

bool isprime(int a){
    if(a<=1){
        return false;
    }
    else if(a==2||a==3){
        return true;
    }
    else{
        for(int i = 2;i*i<=a;i++){
            if(a%i==0){
                return false;
            }
        }
        return true;
    }
}
int main(){
    int num;
    while(cin>>num){
        if(num==1){
            cout<<2<<endl;
        }
        else{
            long long s = 2;
            int i = 3;
            for(;num>1;i+=2){
                if(isprime(i)){
                    s+=i;
                    num--;
                }
                else{

                }
            }
            cout<<s<<endl;
        }
    }


    return 0;
}


#include<iostream>
#include<string>
using namespace std;
long long ancifang(int a,int n){
    long long s = 1;
    for(int i = 0;i<n;i++){
        s*=a;
    }
    return s;
}
bool isprime(int a){
    if(a<=1){
        return false;
    }
    else if(a==2||a==3){
        return true;
    }
    else{
        if(ancifang(2,a-1)%a==1){
            for(int i = 2;i*i<=a;i++){
                if(a%i==0){
                    return false;
                }
            }
            return true;
        }
        else{
            return false;
        }

    }
}
int main(){
    int num;
    while(cin>>num){
        if(num==1){
            cout<<2<<endl;
        }
        else if(num==0){
            cout<<0<<endl;
        }
        else{
            long long s = 2;
            int  i = 3;
            for(;num>1;i+=2){
                if(isprime(i)){
                    s+=i;
                    num--;
                }
                else{

                }
            }
            cout<<s<<endl;
        }
    }


    return 0;
}
