//while改写
#include <iostream>
#include <string>
using namespace std;

int main()
{
    string str;
    cin>>str;
    decltype(str.size()) n=0;
    while (n<str.size())
        {
            str[n]='X';
            n++;
        }
    cout<<str;
    return 0;
}
//traditional for 改写
#include <iostream>
#include <string>
using namespace std;

int main()
{
    string str;
    cin>>str;
    for(decltype(str.size()) n=0;n<str.size();n++)
        str[n]='X';
    cout<<str;
    return 0;
}
